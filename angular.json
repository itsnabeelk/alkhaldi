{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "alkhaldi": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/alkhaldi",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/style2.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/all.css",
              "src/assets/css/animate.css",
              "src/assets/css/swiper-bundle.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/meanmenu.min.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.6.0.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/progressbar.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/swiper-bundle.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/jquery.waypoints.min.js",
              "src/assets/js/jquery.meanmenu.min.js",
              "src/assets/js/custom.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "alkhaldi:build:production"
            },
            "development": {
              "buildTarget": "alkhaldi:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "alkhaldi:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "4925e4df-710f-421d-8543-fdf201955648"
  }
}
